{
    {{#composerPackageName}}
    "name": "{{{.}}}",
    {{/composerPackageName}}
    {{#artifactVersion}}
    "version": "{{{.}}}",
    {{/artifactVersion}}
    "description": "Official PHP SDK for the Shotstack Cloud Video Editing API",
    "type": "library",
    "keywords": [
        "shotstack",
        "video",
        "video editing",
        "api"
    ],
    "homepage": "{{{artifactUrl}}}",
    "license": "{{{licenseName}}}",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "{{{developerOrganization}}}",
            "homepage": "{{{developerOrganizationUrl}}}"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "{{{escapedInvokerPackage}}}\\" : "{{{srcBasePath}}}/" }
    }
}
